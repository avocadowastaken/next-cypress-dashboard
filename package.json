    "mongodb": "^3.6.3",
    "next": "10.0.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@actions/exec": "^1.0.4",
    "@actions/glob": "^0.1.1",
    "@actions/io": "^1.0.2",
    "@prisma/cli": "^2.15.0",
    "@prisma/sdk": "^2.15.0",
    "@types/http-errors": "^1.8.0",
    "@types/lodash": "^4.14.168",
    "@types/mongodb": "^3.6.3",
