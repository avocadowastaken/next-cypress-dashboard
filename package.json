{
  "name": "next-cypress-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build:next",
    "postinstall": "husky install",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:prisma": "prisma generate --watch",
    "build": "run-p build:*",
    "build:next": "prisma generate && next build",
    "build:actions": "ts-node scripts/build-actions.ts",
    "start": "next start",
    "checks": "run-p check:*",
    "check:types": "prisma generate && tsc",
    "check:format": "prettier . --check",
    "migrate": "ts-node scripts/migrate.ts"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "yarn.lock": "npx yarn-deduplicate"
  },
  "dependencies": {
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.3.0",
    "@material-ui/core": "5.0.0-alpha.32",
    "@material-ui/icons": "5.0.0-alpha.29",
    "@material-ui/lab": "5.0.0-alpha.32",
    "@mdi/js": "5.9.55",
    "@octokit/auth-oauth-app": "4.1.2",
    "@octokit/core": "3.4.0",
    "@prisma/client": "2.21.2",
    "date-fns": "2.21.1",
    "git-url-parse": "11.4.4",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "next": "10.2.0",
    "next-connect": "0.10.1",
    "next-iron-session": "4.1.13",
    "prism-react-renderer": "1.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.15.2",
    "superjson": "1.7.4"
  },
  "devDependencies": {
    "@actions/core": "1.2.7",
    "@actions/exec": "1.0.4",
    "@actions/github": "4.0.0",
    "@actions/glob": "0.1.1",
    "@mapbox/rehype-prism": "0.6.0",
    "@mdx-js/loader": "1.6.22",
    "@next/bundle-analyzer": "10.2.0",
    "@next/mdx": "10.2.0",
    "@types/cookie": "0.4.0",
    "@types/git-url-parse": "9.0.0",
    "@types/lodash": "4.14.168",
    "@types/morgan": "1.9.2",
    "@types/node": "15.0.1",
    "@types/node-fetch": "2.5.10",
    "@types/react": "17.0.4",
    "babel-plugin-direct-import": "0.6.3",
    "cypress": "7.2.0",
    "dotenv": "8.2.0",
    "esbuild": "0.11.16",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "next-prisma-plugin": "0.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "prettier-package-json": "2.1.3",
    "prettier-plugin-organize-imports": "1.1.1",
    "prettier-plugin-prisma": "0.8.0",
    "prisma": "2.21.2",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "yaml": "1.10.2"
  }
}
